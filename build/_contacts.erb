<h3 class="section-header">Have a Question?</h3>
<% data.Contacts.each do |row| %>
  <div class="contact-info">
    <h5><%= row['department'] %></h5>
    <h4><%= row['name'] %></h4>
    <p class="italic"><%= row['title'] %></p>
    <p><a href="+tel:<%= row['phone'] %>"><%= row['phone'] %></a></p>
    <p><a href="mailto:<%= row['email'] %>"><%= row['email'] %> <i class="fa fa-chevron-right"></i></a></p>
  </div>
<% end %>
